const func color: charColor (in char: ch) is func
  result
    var color: result is black;
  begin
    case ch of
      when {' '}: result := black;
      when {'R'}: result := light_red;
      when {'G'}: result := light_green;
      when {'B'}: result := light_blue;
      when {'Y'}: result := yellow;
      when {'M'}: result := light_magenta;
      when {'O'}: result := orange;
      when {'r'}: result := dark_red;
      when {'g'}: result := dark_green;
      when {'c'}: result := light_cyan;
      when {'X'}: result := white;
      when {'x'}: result := light_gray;
      when {'b'}: result := brown;
      otherwise:  result := black;
    end case;
  end func;


const proc: drawPic (inout PRIMITIVE_WINDOW: pixmap,
    in integer: xPos, in integer: yPos, in array string: pattern,
    in integer: scale) is func
  local
    var integer: line is 0;
    var integer: column is 0;
    var char: ch is ' ';
    var color: col is black;
  begin
    for line range 1 to length(pattern) do
      for column range 1 to length(pattern[1]) do
        ch := pattern[line][column];
        if ch <> 'X' then
          col := charColor(ch);
          rect(pixmap, (xPos + column) * scale, (yPos + line) * scale,
              scale, scale, col);
        end if;
      end for;
    end for;
  end func;


const proc: drawPic2 (inout PRIMITIVE_WINDOW: pixmap,
    in integer: xPos, in integer: yPos, in array string: pattern,
    in integer: scale) is func
  local
    var integer: height is 0;
    var integer: width is 0;
    var integer: line is 0;
    var integer: column is 0;
    var char: ch is ' ';
    var color: col is black;
  begin
    height := height(pixmap) div scale;
    width := width(pixmap) div scale;
    for line range 1 to length(pattern) do
      for column range 1 to length(pattern[1]) do
        ch := pattern[line][column];
        if ch <> 'X' then
          col := charColor(ch);
          rect(pixmap, (xPos + column) * scale, (yPos + line) * scale,
              scale, scale, col);
          rect(pixmap, pred(width - xPos - column) * scale,
              pred(height - yPos - line) * scale,
              scale, scale, col);
        end if;
      end for;
    end for;
  end func;


const func PRIMITIVE_WINDOW: genPixmap (in array string: pattern,
    in array string: small_card_sign, in array string: color_sign,
    in integer: scale) is func
  result
    var PRIMITIVE_WINDOW: result is PRIMITIVE_WINDOW.value;
  local
    var integer: height is 0;
    var integer: width is 0;
  begin
    height := 2 * (length(pattern) + 12);
    width := length(pattern[1]) + 2 * 13;
    result := newPixmap(width * scale, height * scale);
    clear(result, black);
    rectTo(result, 1 * scale, 1 * scale,
        pred(pred(width) * scale),
        pred(pred(height) * scale), white);
    rect(result, scale, scale,
        scale, scale, light_gray);
    rect(result, (width - 2) * scale, scale,
        scale, scale, light_gray);
    rect(result, scale, (height - 2) * scale,
        scale, scale, light_gray);
    rect(result, (width - 2) * scale, (height - 2) * scale,
        scale, scale, light_gray);
    rectTo(result, 12 * scale, 11 * scale,
        pred((width - 12) * scale),
        pred((height - 11) * scale), black);
    rectTo(result, 13 * scale, 12 * scale,
        pred((width - 13) * scale),
        pred((height - 12) * scale), white);
    drawPic2(result, 12, 11, pattern, scale);
    drawPic2(result,  1,  3, small_card_sign, scale);
    drawPic2(result,  1, 18, color_sign, scale);
  end func;


const func PRIMITIVE_WINDOW: genPixmap (in array string: big_card_sign,
    in array string: small_card_sign, in array string: color_sign,
    in posType: positions, in integer: scale) is func
  result
    var PRIMITIVE_WINDOW: result is PRIMITIVE_WINDOW.value;
  local
    var integer: height is 0;
    var integer: width is 0;
    var integer: line is 0;
    var integer: column is 0;
    var color: col is black;
    var integer: index is 0;
  begin
    height := 96;
    width := 71;
    result := newPixmap(width * scale, height * scale);
    clear(result, black);
    rectTo(result, 1 * scale, 1 * scale,
        pred(pred(width) * scale),
        pred(pred(height) * scale), white);
    rect(result, scale, scale,
        scale, scale, light_gray);
    rect(result, (width - 2) * scale, scale,
        scale, scale, light_gray);
    rect(result, scale, (height - 2) * scale,
        scale, scale, light_gray);
    rect(result, (width - 2) * scale, (height - 2) * scale,
        scale, scale, light_gray);
    for index range 1 to length(positions) do
      if positions[index][3] = 2 then
        drawPic2(result, positions[index][1], positions[index][2],
            big_card_sign, scale);
      else
        drawPic(result, positions[index][1], positions[index][2],
            big_card_sign, scale);
      end if;
    end for;
    drawPic2(result,  1,  3, small_card_sign, scale);
    drawPic2(result,  1, 18, color_sign, scale);
  end func;



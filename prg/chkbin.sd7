$ include "seed7_05.s7i";
  include "stdio.s7i";
  include "bin32.s7i";
  include "bin64.s7i";
  include "bigint.s7i";


const func integer: intExpr (in integer: number) is
  return number + length(str(rand(1, 9))[2 ..]);


const func bin32: bin32Expr (in bin32: aBin32) is
  return bin32(ord(aBin32) + length(str(rand(1, 9))[2 ..]));


const func bin64: bin64Expr (in bin64: aBin64) is
  return bin64(ord(aBin64) + length(str(rand(1, 9))[2 ..]));


const func boolean: boolExpr (in boolean: okay) is
  return boolean(ord(okay) + length(str(rand(1, 9))[2 ..]));


const func bigInteger: bigintExpr (in bigInteger: number) is func
  result
    var bigInteger: exprResult is 0_;
  begin
    exprResult := number;
  end func;


const func boolean: raisesRangeError (ref func bin32: expression) is func
  result
    var boolean: raisesRangeError is FALSE;
  local
    var bin32: exprResult is bin32(0);
  begin
    block
      exprResult := expression;
    exception
      catch RANGE_ERROR:
        raisesRangeError := TRUE;
    end block;
  end func;


const func boolean: raisesRangeError (ref func bin64: expression) is func
  result
    var boolean: raisesRangeError is FALSE;
  local
    var bin64: exprResult is bin64(0);
  begin
    block
      exprResult := expression;
    exception
      catch RANGE_ERROR:
        raisesRangeError := TRUE;
    end block;
  end func;


const func boolean: raisesRangeError (ref func string: expression) is func
  result
    var boolean: raisesRangeError is FALSE;
  local
    var string: exprResult is "";
  begin
    block
      exprResult := expression;
    exception
      catch RANGE_ERROR:
        raisesRangeError := TRUE;
    end block;
  end func;


const proc: checkShift is func
  local
    var boolean: okay is TRUE;
  begin
    if  bin64(1) << 61 <> bin64(2305843009213693952) or
        bin64(1) << 62 <> bin64(4611686018427387904) or
        bin64(1) << 63 <> bin64(9223372036854775808_) then
      okay := FALSE;
    end if;

    if  bin64( 2305843009213693951)  >> 60 <> bin64(1) or
        bin64( 2305843009213693951)  >> 61 <> bin64(0) or
        bin64( 2305843009213693952)  >> 61 <> bin64(1) or
        bin64( 4611686018427387903)  >> 61 <> bin64(1) or
        bin64( 4611686018427387903)  >> 62 <> bin64(0) or
        bin64( 4611686018427387904)  >> 62 <> bin64(1) or
        bin64( 9223372036854775807)  >> 62 <> bin64(1) or
        bin64( 9223372036854775807)  >> 63 <> bin64(0) or
        bin64( 9223372036854775808_) >> 63 <> bin64(1) or
        bin64(18446744073709551615_) >> 63 <> bin64(1) then
      okay := FALSE;
    end if;

    if  bin64(1) << rand(0, 62) = bin64(4611686018427387905) then
      okay := FALSE;
    end if;

    if  bin64(9223372036854775807) >> rand(0, 62) = bin64(0) then
      okay := FALSE;
    end if;

    if okay then
      writeln("Bin64 shift works correct.");
    else
      writeln(" ***** Bin64 shift does not work correct.");
      writeln;
    end if;
  end func;



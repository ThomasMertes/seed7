    const bigInteger: big257m   is   -257_;
    const bigInteger: big65535m is -65535_;
    const bigInteger: big65536m is -65536_;
    const bigInteger: big65537m is -65537_;
  begin
    if  big0      <>      0_ or
        big1      <>      1_ or
        big2      <>      2_ or
        big127    <>    127_ or
        big128    <>    128_ or
        big129    <>    129_ or
        big255    <>    255_ or
        big256    <>    256_ or

(**
 *  Test the case where source and destination of an assignment
 *  are identical.
 *)
const proc: do_assign (inout bigInteger: dest, ref bigInteger: source) is func
  begin
    dest := source;
  end func;


const proc: chkAssign is func
  local
    var boolean: okay is TRUE;
    var bigInteger: number is 1_;
  begin
    okay := TRUE;
    number := 2_;
    if number <> 2_ then
      writeln(" ***** 'number := 2_' does not work correct.");
      okay := FALSE;
    end if;
    number := number;
    if number <> 2_ then
      writeln(" ***** bigInteger 'number := number' does not work correct.");
      okay := FALSE;
    end if;
    do_assign(number, number);
    if number <> 2_ then
      writeln(" ***** bigInteger 'number := number' does not work correct.");
      okay := FALSE;
    end if;
    if okay then
      writeln("BigInteger assignment works correct.");
    end if;
  end func;


const func string: randDigitStri (in integer: length) is func
  result
    var string: digitStri is "";
  local
    var integer: pos is 0;
  begin
    digitStri := " " mult length;
    digitStri @:= [1] rand('1', '9');
    for pos range 2 to length do
      digitStri @:= [pos] rand('0', '9');
    end for;
  end func;


const proc: testConversion (in integer: length, in integer: count) is func
  local
    var integer: number is 0;
    var string: stri is "";
  begin
    if rand(FALSE, TRUE) then
      stri := randDigitStri(length);
    else
      stri := "-" & randDigitStri(length);
    end if;
    for number range 1 to count do
      # ignore(bigInteger parse stri);
      if str(bigInteger parse stri) <> stri then
        writeln(" **** Conversion error with " <& stri);
      end if;
    end for;
  end func;


const proc: testConversion (in integer: count) is func
  local
    var integer: length is 0;
  begin
    for length range 9990 to 10000 do
      testConversion (length, count);
      # writeln(length);
    end for;
  end func;



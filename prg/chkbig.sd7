const func string: randDigitStri (in integer: length) is func
  result
    var string: result is "";
  local
    var integer: pos is 0;
  begin
    result := " " mult length;
    result @:= [1] rand('1', '9');
    for pos range 2 to length do
      result @:= [pos] rand('0', '9');
    end for;
  end func;


const proc: testConversion (in integer: length, in integer: count) is func
  local
    var integer: number is 0;
    var string: stri is "";
  begin
    if rand(FALSE, TRUE) then
      stri := randDigitStri(length);
    else
      stri := "-" & randDigitStri(length);
    end if;
    for number range 1 to count do
      # ignore(bigInteger parse stri);
      if str(bigInteger parse stri) <> stri then
        writeln(" **** Conversion error with " <& stri);
      end if;
    end for;
  end func;


const proc: testConversion (in integer: count) is func
  local
    var integer: length is 0;
  begin
    for length range 9990 to 10000 do
      testConversion (length, count);
      writeln(length);
    end for;
  end func;



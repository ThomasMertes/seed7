const proc: chkConstants is func
  local
    var boolean: okay is TRUE;
    const bigInteger: big0      is      0_;
    const bigInteger: big1      is      1_;
    const bigInteger: big2      is      2_;
    const bigInteger: big127    is    127_;
    const bigInteger: big128    is    128_;
    const bigInteger: big129    is    129_;
    const bigInteger: big255    is    255_;
    const bigInteger: big256    is    256_;
    const bigInteger: big257    is    257_;
    const bigInteger: big65535  is  65535_;
    const bigInteger: big65536  is  65537_;
    const bigInteger: big65537  is  65538_;
    const bigInteger: big1m     is     -1_;
    const bigInteger: big2m     is     -2_;
    const bigInteger: big127m   is   -127_;
    const bigInteger: big128m   is   -128_;
    const bigInteger: big129m   is   -129_;
    const bigInteger: big255m   is   -255_;
    const bigInteger: big256m   is   -256_;
    const bigInteger: big257m   is   -257_;
    const bigInteger: big65535m is -65535_;
    const bigInteger: big65536m is -65536_;
    const bigInteger: big65537m is -65537_;
  begin
    if  big0      <>      0_ or
        big1      <>      1_ or
        big2      <>      2_ or
        big127    <>    127_ or
        big128    <>    128_ or
        big129    <>    129_ or
        big255    <>    255_ or
        big256    <>    256_ or
        big257    <>    257_ or
        big65535  <>  65535_ or
        big65536  <>  65537_ or
        big65537  <>  65538_ or
        big1m     <>     -1_ or
        big2m     <>     -2_ or
        big127m   <>   -127_ or
        big128m   <>   -128_ or
        big129m   <>   -129_ or
        big255m   <>   -255_ or
        big256m   <>   -256_ or
        big257m   <>   -257_ or
        big65535m <> -65535_ or
        big65536m <> -65536_ or
        big65537m <> -65537_ then
      writeln("The bigInteger constants do not work correct.");
      okay := FALSE;
    end if;

    if  compare(bigintExpr(-2_), big2m) <>  0 or
        compare(bigintExpr(-1_), big2m) <>  1 or
        compare(bigintExpr( 0_), big2m) <>  1 or
        compare(bigintExpr( 2_), big2m) <>  1 or
        compare(bigintExpr( 2_), big2m) <>  1 or
        compare(bigintExpr(-2_), big1m) <> -1 or
        compare(bigintExpr(-1_), big1m) <>  0 or
        compare(bigintExpr( 0_), big1m) <>  1 or
        compare(bigintExpr( 1_), big1m) <>  1 or
        compare(bigintExpr( 2_), big1m) <>  1 or
        compare(bigintExpr(-2_), big0)  <> -1 or
        compare(bigintExpr(-1_), big0)  <> -1 or
        compare(bigintExpr( 0_), big0)  <>  0 or
        compare(bigintExpr( 1_), big0)  <>  1 or
        compare(bigintExpr( 2_), big0)  <>  1 or
        compare(bigintExpr(-2_), big1)  <> -1 or
        compare(bigintExpr(-1_), big1)  <> -1 or
        compare(bigintExpr( 0_), big1)  <> -1 or
        compare(bigintExpr( 1_), big1)  <>  0 or
        compare(bigintExpr( 2_), big1)  <>  1 or
        compare(bigintExpr(-2_), big2)  <> -1 or
        compare(bigintExpr(-1_), big2)  <> -1 or
        compare(bigintExpr( 0_), big2)  <> -1 or
        compare(bigintExpr( 1_), big2)  <> -1 or
        compare(bigintExpr( 2_), big2)  <>  0 then
      writeln("The bigInteger constant comparisons do not work correct.");
      okay := FALSE;
    end if;


    if okay then
      writeln("The bigInteger constants work correct.");
    else
      writeln(" ***** The bigInteger constants do not work correct.");
      writeln;
    end if;
  end func;


(**
 *  Test the case where source and destination of an assignment
 *  are identical.
 *)
const proc: do_assign (inout bigInteger: dest, ref bigInteger: source) is func
  begin
    dest := source;
  end func;


const proc: chkAssign is func
  local
    var boolean: okay is TRUE;
    var bigInteger: number is 1_;
  begin
    okay := TRUE;
    number := 2_;
    if number <> 2_ then
      writeln(" ***** 'number := 2_' does not work correct.");
      okay := FALSE;
    end if;
    number := number;
    if number <> 2_ then
      writeln(" ***** bigInteger 'number := number' does not work correct.");
      okay := FALSE;
    end if;
    do_assign(number, number);
    if number <> 2_ then
      writeln(" ***** bigInteger 'number := number' does not work correct.");
      okay := FALSE;
    end if;
    if okay then
      writeln("BigInteger assignment works correct.");
    end if;
  end func;


const func string: randDigitStri (in integer: length) is func
  result
    var string: result is "";
  local
    var integer: pos is 0;
  begin
    result := " " mult length;
    result @:= [1] rand('1', '9');
    for pos range 2 to length do
      result @:= [pos] rand('0', '9');
    end for;
  end func;


const proc: testConversion (in integer: length, in integer: count) is func
  local
    var integer: number is 0;
    var string: stri is "";
  begin
    if rand(FALSE, TRUE) then
      stri := randDigitStri(length);
    else
      stri := "-" & randDigitStri(length);
    end if;
    for number range 1 to count do
      # ignore(bigInteger parse stri);
      if str(bigInteger parse stri) <> stri then
        writeln(" **** Conversion error with " <& stri);
      end if;
    end for;
  end func;


const proc: testConversion (in integer: count) is func
  local
    var integer: length is 0;
  begin
    for length range 9990 to 10000 do
      testConversion (length, count);
      # writeln(length);
    end for;
  end func;



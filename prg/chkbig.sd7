        big127m   <>   -127_ or
        big128m   <>   -128_ or
        big129m   <>   -129_ or
        big255m   <>   -255_ or
        big256m   <>   -256_ or
        big257m   <>   -257_ or
        big65535m <> -65535_ or
        big65536m <> -65536_ or
        big65537m <> -65537_ then
      writeln("The bigInteger constants do not work correct.");
      okay := FALSE;
    end if;

    if  compare(bigintExpr(-2_), big2m) <>  0 or
        compare(bigintExpr(-1_), big2m) <>  1 or

    in bigInteger: checkValue) is func
  result
    var boolean: wrong is FALSE;
  local
    var bigInteger: bigVar is 0_;
  begin
    wrong := big1 <<  lshift <> checkValue or
             big1 >> -lshift <> checkValue or
             bigintExpr(big1) <<  lshift <> checkValue or
             bigintExpr(big1) >> -lshift <> checkValue;

const func boolean: relationTest (in bitset: set1, in bitset: set2,
    in boolean: expectedResult, in boolean: setsAreEqual) is
  return (set1 >= set2) <> expectedResult or
         (set2 <= set1) <> expectedResult or
         (set1 > set2) <> (expectedResult and not setsAreEqual) or
         (set2 < set1) <> (expectedResult and not setsAreEqual) or
         (set1 = set2) <> setsAreEqual or
         (set2 = set1) <> setsAreEqual or
         (set1 <> set2) = setsAreEqual or
         (set2 <> set1) = setsAreEqual;



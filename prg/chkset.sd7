const proc: check_min is func
  local
    var boolean: okay is TRUE;
    var integer: number is 0;
    var integer: num2 is 0;
    var integer: random_num is 0;
    var bitset: test_set is EMPTY_SET;
  begin
    okay := TRUE;
    for number range -512 to 512 do
      test_set := EMPTY_SET;
      incl(test_set, number);
      for num2 range 1 to 128 do
        random_num := rand(succ(number), 513);
        incl(test_set, random_num);
      end for;
      if min(test_set)    <> number then
        write("min(bitset) returns ");
        write(min(test_set));
        write(" instead of ");
        write(number);
        writeln(".");
        okay := FALSE;
      end if;
    end for;

    if okay then
      writeln("min(bitset) works correct.");
    else
      writeln(" ***** min(set) does not work correct");
      writeln;
    end if;
  end func;



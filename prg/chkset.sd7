  begin
    if  bitset(                  0) <> EMPTY_SET or
        bitset(                  1) <> {0} or
        bitset(                  2) <> {1} or
        bitset(                  3) <> {0, 1} or
        bitset(                363) <> {0, 1, 3, 5, 6, 8} or
        bitset(            2105647) <> {0, 1, 2, 3, 5, 8, 13, 21} or
        bitset(          205063038) <> {1, 2, 3, 4, 5, 6, 8, 9, 16, 19, 20, 21, 26, 27} or
        bitset(5906320444445337516) <> {2, 3, 5, 7, 8, 9, 10, 13, 15, 17, 19, 22, 23,
                                        26, 28, 29, 31, 33, 34, 37,  40, 41, 42, 44,
                                        45, 46, 48, 49, 50, 52, 53, 54, 55, 56, 60, 62} or
        bitset(       integer.last) <> {0 .. 62} or
        bitset conv                   0 <> EMPTY_SET or

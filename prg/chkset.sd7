const type: bool_arr is array boolean;


const proc: check_card is func
  local
    var boolean: okay is TRUE;
    var integer: size_of_set is 0;
    var integer: number is 0;
    var integer: random_num is 0;
    var bitset: test_set is EMPTY_SET;
  begin
    okay := TRUE;
    for size_of_set range 0 to 128 do
      test_set := EMPTY_SET;
      for number range 1 to size_of_set do
        repeat
          random_num := rand(-128, 128);
        until random_num not in test_set;
        incl(test_set, random_num);
      end for;
      if card(test_set) <> size_of_set then
        write("card(bitset) returns ");
        write(card(test_set));
        write(" instead of ");
        write(size_of_set);
        writeln(".");
        okay := FALSE;
      end if;
    end for;

    if okay then
      writeln("card(bitset) works correct.");
    else
      writeln(" ***** card(set) does not work correct");
      writeln;
    end if;
  end func;


const proc: check_incl is func
  local
    var boolean: okay is TRUE;
    var integer: size_of_set is 0;
    var integer: number is 0;
    var integer: random_num is 0;
    var bitset: test_set is EMPTY_SET;
    var array boolean: compare_set is 0 times FALSE;
  begin
    okay := TRUE;
    for size_of_set range 0 to 128 do
      test_set := EMPTY_SET;
      compare_set := 257 times FALSE;
      for number range 1 to size_of_set do
        repeat
          random_num := rand(-128, 128);
        until random_num not in test_set;
        incl(test_set, random_num);
        compare_set[random_num + 129] := TRUE;
      end for;
      for number range -128 to 128 do
        if compare_set[number + 129] then
          if not number in test_set then
            write("incl(bitset, ");
            write(number);
            writeln(") does not include element to bitset.");
            okay := FALSE;
          end if;
        else
          if number in test_set then
            write("bit ");
            write(number);
            writeln(" set by incl(bitset, integer) although not called with this number.");
            okay := FALSE;
          end if;
        end if;
      end for;
    end for;

    if okay then
      writeln("incl(bitset, integer) works correct.");
    else
      writeln(" ***** incl(bitset, integer) does not work correct");
      writeln;
    end if;
  end func;


const func boolean: compareTest (in bitset: set1, in bitset: set2,
    in integer: expectedResult) is
  return compare(set1, set2) <> expectedResult and
         compare(set2, set1) <> -expectedResult;



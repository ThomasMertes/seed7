        incl(test_set, random_num);
      end for;
      if max(test_set) <> number then
        write("max(bitset) returns ");
        write(max(test_set));
        write(" instead of ");
        write(number);
        writeln(".");
        okay := FALSE;
      end if;
    end for;

    if okay then
      writeln("max(bitset) works correct.");
    else
      writeln(" ***** max(set) does not work correct");
      writeln;
    end if;
  end func;



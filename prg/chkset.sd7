    end if;

    if bitset(alphanum_char) <> bitset(letter_char | digit_char) then
      writeln("Conversion of character set to integer set does not work correctly.");
      okay := FALSE;
    end if;

    if okay then
      writeln("character sets work correctly.");
    else
      writeln(" ***** character sets do not work correctly");
      writeln;
    end if;
  end func;



const proc: check_card is func
  local
    var boolean: okay is TRUE;
    var integer: size_of_set is 0;
    var integer: number is 0;
    var integer: random_num is 0;
    var bitset: test_set is EMPTY_SET;
    var string: random_stri is "";
    var set of string: test_set2 is (set of string).EMPTY_SET;
    var integer: count is 0;
  begin
    for size_of_set range 0 to 128 do
      test_set := EMPTY_SET;
      for number range 1 to size_of_set do
        repeat
          random_num := rand(-128, 128);
        until random_num not in test_set;
        incl(test_set, random_num);
      end for;
      if card(test_set) <> size_of_set then
        writeln("card(bitset) returns " <& card(test_set) <&
                " instead of " <& size_of_set <& ".");
        okay := FALSE;
      end if;
    end for;

    for size_of_set range 0 to 128 do
      test_set2 := (set of string).EMPTY_SET;
      for number range 1 to size_of_set do
        repeat
          random_stri := str(rand(-128, 128));
        until random_stri not in test_set2;
        incl(test_set2, random_stri);
      end for;
      if card(test_set2) <> size_of_set then
        writeln("card(hashset) returns " <& card(test_set2) <&
                " instead of " <& size_of_set <& ".");
        okay := FALSE;
      end if;
    end for;

    for count range 1 to 10000 do
      number := rand(0, integer.last);
      if  card(bitset(number)) <> length(replace(number radix 2, "0", "")) or
          card(bitset conv number) <> length(replace(number radix 2, "0", "")) then
        writeln("card(bitset(" <& number <& ")) returns " <& card(bitset(number)) <&
                " instead of " <& length(replace(number radix 2, "0", "")) <& ".");
        okay := FALSE;
      end if;
    end for;

    number := -1;
    if  card(bitset(1)) <> 1 or
        card(bitset(10)) <> 2 or
        card(bitset(abs(number))) <> 1 or
        card(bitset conv 1) <> 1 or
        card(bitset conv 10) <> 2 or
        card(bitset conv abs(number)) <> 1 or
        card(bitset(intExpr(1))) <> 1 or
        card(bitset(intExpr(10))) <> 2 or
        card(bitset(intExpr(abs(number)))) <> 1 or
        card(bitset conv intExpr(1)) <> 1 or
        card(bitset conv intExpr(10)) <> 2 or
        card(bitset conv intExpr(abs(number))) <> 1 then
      writeln("card(bitset(1)) does not return 1.");
      okay := FALSE;
    end if;

    if  card(bitset(intExpr(0) mod 1))     <> 0 or
        card(bitset(intExpr(0) mod 1 + 1)) <> 1 or
        card(bitset(intExpr(0) mod 1 + 2)) <> 1 or
        card(bitset(intExpr(0) mod 1 + 3)) <> 2 then
      writeln("card(bitset(n)) does not work correctly.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(bitset(-1)) or
        not raisesRangeError(bitset(number)) or
        not raisesRangeError(bitset conv (-1)) or
        not raisesRangeError(bitset conv number) or
        not raisesRangeError(bitset(intExpr(-1))) or
        not raisesRangeError(bitset(intExpr(number))) or
        not raisesRangeError(bitset conv intExpr(-1)) or
        not raisesRangeError(bitset conv intExpr(number)) then
      writeln("bitset(-1) does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(card(bitset(-1))) or
        not raisesRangeError(card(bitset(number))) or
        not raisesRangeError(card(bitset conv (-1))) or
        not raisesRangeError(card(bitset conv number)) or
        not raisesRangeError(card(bitset(intExpr(-1)))) or
        not raisesRangeError(card(bitset(intExpr(number)))) or
        not raisesRangeError(card(bitset conv intExpr(-1))) or
        not raisesRangeError(card(bitset conv intExpr(number))) then
      writeln("card(bitset(-1)) does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if okay then
      writeln("card(set) works correctly.");
    else
      writeln(" ***** card(set) does not work correctly");
      writeln;
    end if;
  end func;


const proc: check_rand is func
  local
    var boolean: okay is TRUE;
    var integer: size_of_set is 0;
    var integer: number is 0;
    var integer: random_num is 0;
    var bitset: test_set is EMPTY_SET;
    var string: random_stri is "";
    var set of string: test_set2 is (set of string).EMPTY_SET;
  begin
    for size_of_set range 0 to 128 do
      test_set := EMPTY_SET;
      for number range 1 to size_of_set do
        repeat
          random_num := rand(-128, 128);
        until random_num not in test_set;
        incl(test_set, random_num);
      end for;
      for number range 1 to size_of_set do
        random_num := rand(test_set);
        if random_num not in test_set then
          writeln(" ***** rand(bitset) returns something that is not in the set.");
          okay := FALSE;
        end if;
        excl(test_set, random_num);
      end for;
      if test_set <> EMPTY_SET then
        writeln(" ***** rand(bitset) should return all elements of a set.");
        okay := FALSE;
      end if;
    end for;

    for size_of_set range 0 to 128 do
      test_set2 := (set of string).EMPTY_SET;
      for number range 1 to size_of_set do
        repeat
          random_stri := str(rand(-128, 128));
        until random_stri not in test_set2;
        incl(test_set2, random_stri);
      end for;
      for number range 1 to size_of_set do
        random_stri := rand(test_set2);
        if random_stri not in test_set2 then
          writeln(" ***** rand(hashset) returns something that is not in the set.");
          okay := FALSE;
        end if;
        excl(test_set2, random_stri);
      end for;
      if test_set2 <> (set of string).EMPTY_SET then
        writeln(" ***** rand(hashset) should return all elements of a set.");
        okay := FALSE;
      end if;
    end for;

    if  rand({-1234567890}) <> -1234567890 or
        rand({         -1}) <>          -1 or
        rand({          0}) <>           0 or
        rand({          1}) <>           1 or
        rand({ 1234567890}) <>  1234567890 then
      writeln(" ***** rand() of a set with one element does not return this element.");
      okay := FALSE;
    end if;

    if  rand({1, 2})       not in {1, 2} or
        rand({1, 2, 3})    not in {1, 2, 3} or
        rand({1, 2, 3, 4}) not in {1, 2, 3, 4} then
      writeln(" ***** rand() of a set with consecutive elements does not work correctly.");
      okay := FALSE;
    end if;

    if  rand({2, 3, 5})        not in {2, 3, 5} or
        rand({2, 3, 5, 7})     not in {2, 3, 5, 7} or
        rand({2, 3, 5, 7, 11}) not in {2, 3, 5, 7, 11} then

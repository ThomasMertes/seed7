$ include "seed7_05.s7i";
  include "stdio.s7i";
  include "bin64.s7i";


const set of char: uc_letter_char is {'A' .. 'Z'};
const set of char: lc_letter_char is {'a' .. 'z'};
const set of char: letter_char is {'A' .. 'Z'} | {'a' .. 'z'};
const set of char: digit_char is {'0' .. '9'};
const set of char: alphanum_char is letter_char | digit_char;
const set of char: special_char is
    {'!', '$', '%', '&', '*', '+', ',', '-', '.', '/',
     ':', ';', '<', '=', '>', '?', '@', '\\','^', '`',
     '|', '~'};
const set of char: name_start_char is letter_char | {'_'};
const set of char: name_char is letter_char | digit_char | {'_'};

const type: striSet is set of string;

const type: bool_arr is array boolean;


const func integer: intExpr (in integer: number) is
  return number + length(str(rand(1, 9))[2 ..]);


const func boolean: boolExpr (ref boolean: value) is
  return value and str(rand(1, 9))[2 ..] = "";


const func boolean: raisesRangeError (ref func bitset: expression) is func
  result
    var boolean: raisesRangeError is FALSE;
  local
    var bitset: exprResult is bitset.value;
  begin
    block
      exprResult := expression;
    exception
      catch RANGE_ERROR:
        raisesRangeError := TRUE;
    end block;
  end func;


const func boolean: raisesRangeError (ref func integer: expression) is func
  result
    var boolean: raisesRangeError is FALSE;
  local
    var integer: exprResult is 0;
  begin
    block
      exprResult := expression;
    exception
      catch RANGE_ERROR:
        raisesRangeError := TRUE;
    end block;
  end func;


const proc: check_literal is func
  local
    var boolean: okay is TRUE;
  begin
    if  {-9223372036854775807 ..        integer.first} <> EMPTY_SET or
        {                  -1 ..                   -2} <> EMPTY_SET or
        {                   0 ..                   -1} <> EMPTY_SET or
        {                   1 ..                    0} <> EMPTY_SET or
        {        integer.last ..  9223372036854775806} <> EMPTY_SET or
        {       integer.first ..        integer.first} <> {integer.first} or
        {                  -1 ..                   -1} <> {-1} or
        {                   0 ..                    0} <> {0} or
        {        integer.last ..         integer.last} <> {integer.last} or
        {       integer.first .. -9223372036854775807} <> {integer.first, -9223372036854775807} or
        {                  -2 ..                   -1} <> {-2, -1} or
        {                  -1 ..                    0} <> {-1, 0} or
        {                   0 ..                    1} <> {0, 1} or
        { 9223372036854775806 ..         integer.last} <> {9223372036854775806, integer.last} or
        {-9223372036854775807 .. intExpr(       integer.first)} <> EMPTY_SET or
        {                  -1 .. intExpr(                  -2)} <> EMPTY_SET or
        {                   0 .. intExpr(                  -1)} <> EMPTY_SET or
        {                   1 .. intExpr(                   0)} <> EMPTY_SET or
        {        integer.last .. intExpr( 9223372036854775806)} <> EMPTY_SET or
        {       integer.first .. intExpr(       integer.first)} <> {integer.first} or
        {                  -1 .. intExpr(                  -1)} <> {-1} or
        {                   0 .. intExpr(                   0)} <> {0} or
        {        integer.last .. intExpr(        integer.last)} <> {integer.last} or
        {       integer.first .. intExpr(-9223372036854775807)} <> {integer.first, -9223372036854775807} or
        {                  -2 .. intExpr(                  -1)} <> {-2, -1} or
        {                  -1 .. intExpr(                   0)} <> {-1, 0} or
        {                   0 .. intExpr(                   1)} <> {0, 1} or
        { 9223372036854775806 .. intExpr(        integer.last)} <> {9223372036854775806, integer.last} or
        {intExpr(-9223372036854775807) ..        integer.first} <> EMPTY_SET or
        {intExpr(                  -1) ..                   -2} <> EMPTY_SET or
        {intExpr(                   0) ..                   -1} <> EMPTY_SET or
        {intExpr(                   1) ..                    0} <> EMPTY_SET or
        {intExpr(        integer.last) ..  9223372036854775806} <> EMPTY_SET or
        {intExpr(       integer.first) ..        integer.first} <> {integer.first} or
        {intExpr(                  -1) ..                   -1} <> {-1} or
        {intExpr(                   0) ..                    0} <> {0} or
        {intExpr(        integer.last) ..         integer.last} <> {integer.last} or
        {intExpr(       integer.first) .. -9223372036854775807} <> {integer.first, -9223372036854775807} or
        {intExpr(                  -2) ..                   -1} <> {-2, -1} or
        {intExpr(                  -1) ..                    0} <> {-1, 0} or
        {intExpr(                   0) ..                    1} <> {0, 1} or
        {intExpr( 9223372036854775806) ..         integer.last} <> {9223372036854775806, integer.last} or
        {intExpr(-9223372036854775807) .. intExpr(       integer.first)} <> EMPTY_SET or
        {intExpr(                  -1) .. intExpr(                  -2)} <> EMPTY_SET or
        {intExpr(                   0) .. intExpr(                  -1)} <> EMPTY_SET or
        {intExpr(                   1) .. intExpr(                   0)} <> EMPTY_SET or
        {intExpr(        integer.last) .. intExpr( 9223372036854775806)} <> EMPTY_SET or
        {intExpr(       integer.first) .. intExpr(       integer.first)} <> {integer.first} or
        {intExpr(                  -1) .. intExpr(                  -1)} <> {-1} or
        {intExpr(                   0) .. intExpr(                   0)} <> {0} or
        {intExpr(        integer.last) .. intExpr(        integer.last)} <> {integer.last} or
        {intExpr(       integer.first) .. intExpr(-9223372036854775807)} <> {integer.first, -9223372036854775807} or
        {intExpr(                  -2) .. intExpr(                  -1)} <> {-2, -1} or
        {intExpr(                  -1) .. intExpr(                   0)} <> {-1, 0} or
        {intExpr(                   0) .. intExpr(                   1)} <> {0, 1} or
        {intExpr( 9223372036854775806) .. intExpr(        integer.last)} <> {9223372036854775806, integer.last} then
      writeln(" ***** Bitset literals do not work correctly.");
    end if;

    if okay then
      writeln("Bitset literals work correctly.");
    else
      writeln(" ***** Bitset literals do not work correctly");
      writeln;
    end if;
  end func;


const proc: check_conv is func
  local

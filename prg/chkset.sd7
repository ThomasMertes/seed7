         not (ch     in special_char)    or
              ch not in special_char     or
              ch     in name_start_char  or
         not (ch not in name_start_char) or
              ch     in name_char        or
         not (ch not in name_char)       then
        okay := FALSE;
      end if;
    end for;

    if okay then
      writeln("character sets work correct.");
    else
      writeln(" ***** character sets do not work correct");
      writeln;
    end if;
  end func;



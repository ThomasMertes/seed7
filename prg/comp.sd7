    c_expr.expr &:= "void o_";
    create_name(current_object, c_expr);
    params := params(current_object);
    c_expr.expr &:= " (";
    process_params(params, c_expr);
    c_expr.expr &:= ")\n";
    c_expr.expr &:= "{\n";
    c_expr.expr &:= "arrCpy(";
    process_expr(params[1], c_expr);
    c_expr.expr &:= ", ";
    process_expr(params[3], c_expr);
    c_expr.expr &:= ", ";
    create_func_addr(getType(params[1]), c_expr);

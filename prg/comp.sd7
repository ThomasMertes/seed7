const proc: count_declarations is func

  begin
    incr(declaration_count);
    OUT << declaration_count;
    OUT << " ";
    OUT << heapsize(PROGRAM);
    OUT << "     \r";
    flush(OUT);
  end func;


const proc: create_name (in reference: current_object, inout expr_type: c_expr) is func

  begin
    c_expr.expr &:= str(obj_number(current_object));
    c_expr.expr &:= "/*";
    c_expr.expr &:= str(current_object);
    c_expr.expr &:= "*/";
  end func;


const proc: process_type (in type: object_type, inout expr_type: c_expr) is func

  begin
    if object_type = typetype then
      c_expr.expr &:= "typetype";
    elsif object_type = inttype then
      c_expr.expr &:= "inttype";
    elsif object_type = chartype then
      c_expr.expr &:= "chartype";
    elsif object_type = stritype then
      c_expr.expr &:= "stritype";
    elsif object_type = floattype then
      c_expr.expr &:= "floattype";
    else
      c_expr.expr &:= "t_";
      c_expr.expr &:= str(type_number(object_type));
      c_expr.expr &:= "/*";
      c_expr.expr &:= str(object_type);
      c_expr.expr &:= "*/";
    end if;
  end func;


const proc: process_create_declaration (in type: object_type,
    inout expr_type: c_expr) is forward;


const proc: process_create_call (in type: object_type,
    in string: param_a, in string: param_b, inout expr_type: c_expr) is forward;


const proc: process_arr_create_declaration (in type: object_type,
    inout expr_type: c_expr) is func

  local
    var number_element_hash: elements is number_element_hash.EMPTY_HASH;
    var integer: number is 0;
    var reference: struct_elem is NIL;
    var type: elem_type is void;
  begin
    process_create_declaration(array_element[object_type], c_expr);
    process_type(object_type, c_expr);
    c_expr.expr &:= " create_";
    c_expr.expr &:= str(type_number(object_type));
    c_expr.expr &:= " (";
    process_type(object_type, c_expr);
    c_expr.expr &:= " b)\n";
    c_expr.expr &:= "{\n";
    process_type(object_type, c_expr);
    c_expr.expr &:= " a;\n";
    c_expr.expr &:= "int i;\n";
    c_expr.expr &:= "a=malloc(sizeof(struct arraystruct) - sizeof(long) + ((arraytype)b)->size * sizeof(long));\n";
    c_expr.expr &:= "for (i=0; i<=((arraytype)b)->size; i++) {\n";
    process_create_call(array_element[object_type],
        "((long *)a)[i]", "((long *)b)[i]", c_expr);
    c_expr.expr &:= ";\n";
    c_expr.expr &:= "}\n";
    c_expr.expr &:= "return(a);\n";
    c_expr.expr &:= "}\n\n";
  end func;


const proc: process_sct_create_declaration (in type: object_type,
    inout expr_type: c_expr) is func

  local
    var number_element_hash: elements is number_element_hash.EMPTY_HASH;
    var integer: number is 0;
    var reference: struct_elem is NIL;
    var type: elem_type is void;
  begin
    elements := flip(struct_elements[object_type]);
    for number range 0 to pred(length(elements)) do
      struct_elem := elements[number][1];
      process_create_declaration(struct_element_type[object_type][struct_elem], c_expr);
    end for;
    process_type(object_type, c_expr);
    c_expr.expr &:= " create_";
    c_expr.expr &:= str(type_number(object_type));
    c_expr.expr &:= " (";
    process_type(object_type, c_expr);
    c_expr.expr &:= " b)\n";
    c_expr.expr &:= "{\n";
    process_type(object_type, c_expr);
    c_expr.expr &:= " a;\n";
    c_expr.expr &:= "a=malloc(";
    c_expr.expr &:= str(length(elements));
    c_expr.expr &:= " * sizeof(long));\n";
    for number range 0 to pred(length(elements)) do
      struct_elem := elements[number][1];
      process_create_call(struct_element_type[object_type][struct_elem],
          "((long *)a)[" & str(number) & "]", "((long *)b)[" & str(number) & "]", c_expr);
      c_expr.expr &:= ";\n";
    end for;
    c_expr.expr &:= "return(a);\n";
    c_expr.expr &:= "}\n\n";
  end func;


const proc: process_create_declaration (in type: object_type,
    inout expr_type: c_expr) is func

  begin
    if object_type not in create_declared then
      if object_type in array_element then
        process_arr_create_declaration(object_type, c_expr);
      elsif object_type in struct_elements then
        process_sct_create_declaration(object_type, c_expr);
      else
        c_expr.expr &:= "/* create_declaration for type ";
        c_expr.expr &:= "t_";
        c_expr.expr &:= str(type_number(object_type));
        c_expr.expr &:= "**";
        c_expr.expr &:= str(object_type);
        c_expr.expr &:= "*/\n\n";
      end if;
      create_declared @:= [object_type] TRUE;
    end if;
  end func;


const proc: process_arr_create_call (in type: object_type,
    in string: param_a, in string: param_b, inout expr_type: c_expr) is func

  begin
    c_expr.expr &:= param_a;
    c_expr.expr &:= "=create_";
    c_expr.expr &:= str(type_number(object_type));
    c_expr.expr &:= "(";
    c_expr.expr &:= param_b;
    c_expr.expr &:= ")";
  end func;


const proc: process_sct_create_call (in type: object_type,
    in string: param_a, in string: param_b, inout expr_type: c_expr) is func

  begin
    c_expr.expr &:= param_a;
    c_expr.expr &:= "=create_";

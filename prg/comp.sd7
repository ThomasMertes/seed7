  end func;


const proc: process_create_call (in type: object_type,
    in string: param_a, in string: param_b, inout expr_type: c_expr) is func

  begin
    if object_type = inttype then
      c_expr.expr &:= param_a & "=" & param_b;
    elsif object_type = chartype then
      c_expr.expr &:= param_a & "=" & param_b;
    elsif object_type = stritype then
      c_expr.expr &:= "strCopy(" & param_a & ", " & param_b & ")";
    elsif object_type = floattype then
      c_expr.expr &:= param_a & "=" & param_b;
    elsif object_type in array_element then
      process_arr_create_call(object_type, param_a, param_b, c_expr);
    elsif object_type in struct_elements then
      process_sct_create_call(object_type, param_a, param_b, c_expr);
    else
      c_expr.expr &:= "/* create_ for type ";
      c_expr.expr &:= "t_";
      c_expr.expr &:= str(type_number(object_type));
      c_expr.expr &:= "**";
      c_expr.expr &:= str(object_type);
      c_expr.expr &:= "*/";
    end if;
  end func;


const func string: enum_value (in reference: current_object) is func

  result
    var string: result is "";
  local
    var string: object_name is "";
  begin
    object_name := str(current_object);
    if object_name = "FALSE" then
      result := "0";
    elsif object_name = "TRUE" then
      result := "1";
    else
      result := str(obj_number(current_object));
    end if;
  end func;


const proc: reference_value (in reference: current_value,
    inout expr_type: c_expr) is func

  begin
    if current_value = NIL then
      c_expr.expr &:= "NULL";
    else
      c_expr.expr &:= "&(";
      process_expr(current_value, c_expr);
      c_expr.expr &:= ")";
    end if;
  end func;


const proc: ref_list_value (in ref_list: current_value,
    inout expr_type: c_expr) is func

  local
    var reference: element is NIL;
    var boolean: first_element is TRUE;
  begin
    if length(current_value) = 0 then
      c_expr.expr &:= "NULL";
    else
      c_expr.expr &:= "{";
      for element range current_value do
        if first_element then
          first_element := FALSE;
        else
          c_expr.expr &:= ", ";
        end if;
        c_expr.expr &:= "&(";
        process_expr(element, c_expr);
        c_expr.expr &:= ")";
      end for;
      c_expr.expr &:= "}";
    end if;
  end func;


const proc: process_object_value (in reference: current_object,
    in reference: object_value, inout string: local_init,
    inout expr_type: c_expr) is func

  local
    var string: objclass is "";
  begin
    objclass := obj_class(object_value);
    if objclass = "TYPEOBJECT" then
      c_expr.expr &:= "=NULL";
    elsif objclass = "INTOBJECT" then
      c_expr.expr &:= "=";
      c_expr.expr &:= str(integer value object_value);
    elsif objclass = "BIGINTOBJECT" then
      c_expr.expr &:= "=";
      # c_expr.expr &:= str(bigInteger value object_value);
    elsif objclass = "CHAROBJECT" then
      c_expr.expr &:= "=";
      c_expr.expr &:= c_literal(char value object_value);
    elsif objclass = "STRIOBJECT" then
      if string value object_value not in stri_const_table then
        stri_const_table @:= [string value object_value] length(stri_const_table);
      end if;
      local_init &:= "strCreate(&o_";
      local_init &:= str(obj_number(current_object));
      local_init &:= "/*";
      local_init &:= str(current_object);
      local_init &:= "*/";
      local_init &:= ", str[";
      local_init &:= str(stri_const_table[string value object_value]);
      local_init &:= "] /* ";
      local_init &:= replace(c_literal(string value object_value), "*/", "* /");
      local_init &:= " */";
      local_init &:= ");\n";
    elsif objclass = "FLOATOBJECT" then
      c_expr.expr &:= "=";
      c_expr.expr &:= str(float value object_value);
    elsif objclass = "REFOBJECT" then
      c_expr.expr &:= "=";
      reference_value(reference value object_value, c_expr);
    elsif objclass = "REFLISTOBJECT" then
      c_expr.expr &:= "=";
      ref_list_value(ref_list value object_value, c_expr);
    elsif objclass = "FILEOBJECT" then
      c_expr.expr &:= "=NULL";
    elsif objclass = "ARRAYOBJECT" then
      c_expr.expr &:= "=NULL";
      if current_object not in const_table then
        const_table @:= [current_object] length(const_table);
      end if;
      process_create_declaration(getType(current_object), global_c_expr);
      local_init &:= "o_";
      local_init &:= str(obj_number(current_object));
      local_init &:= "=create_";
      local_init &:= str(type_number(getType(current_object)));
      local_init &:= "(arr[";
      local_init &:= str(const_table[current_object]);
      local_init &:= "]);\n";
    elsif objclass = "HASHOBJECT" then
      c_expr.expr &:= "=NULL";
    elsif objclass = "SETOBJECT" then
      if bitset value object_value not in set_const_table then
        set_const_table @:= [bitset value object_value] length(set_const_table);
      end if;
      local_init &:= "setCreate(&o_";
      local_init &:= str(obj_number(current_object));
      local_init &:= ", set[";
      local_init &:= str(set_const_table[bitset value object_value]);
      local_init &:= "]);\n";
    elsif objclass = "STRUCTOBJECT" then
      if current_object not in const_table then
        const_table @:= [current_object] length(const_table);
      end if;
      process_create_declaration(getType(current_object), global_c_expr);
      local_init &:= "o_";
      local_init &:= str(obj_number(current_object));
      local_init &:= "=create_";
      local_init &:= str(type_number(getType(current_object)));
      local_init &:= "(sct[";
      local_init &:= str(const_table[current_object]);
      local_init &:= "]);\n";
    elsif objclass = "CLASSOBJECT" then
      c_expr.expr &:= "=NULL";
    elsif objclass = "WINOBJECT" then
      c_expr.expr &:= "=NULL";
    elsif objclass = "CONSTENUMOBJECT" then
      c_expr.expr &:= "=";
      c_expr.expr &:= enum_value(reference value object_value);
    elsif objclass = "VARENUMOBJECT" then
      c_expr.expr &:= "=";
      c_expr.expr &:= enum_value(reference value object_value);
    elsif objclass = "ACTOBJECT" then
      c_expr.expr &:= "=NULL";
    else
      c_expr.expr &:= "/* ";
      c_expr.expr &:= objclass;
      c_expr.expr &:= " */";
    end if;
  end func;


const proc: process_locals (in reference: current_object, inout string: local_init,
    inout expr_type: c_expr) is func

  local
    var ref_list: objects is ref_list.EMPTY;
    var reference: obj is NIL;
  begin
    objects := locals(current_object);
    for obj range objects do
      process_type(getType(obj), c_expr);
      c_expr.expr &:= " o_";
      create_name(obj, c_expr);
      process_object_value(obj, reference value obj, local_init, c_expr);
      c_expr.expr &:= ";\n";
    end for;
  end func;


const proc: declare_type_if_necessary (in type: aType, inout expr_type: c_expr) is func

  local
    var reference: type_obj is NIL;
(*
    var reference: expression is NIL;
    var reference: type_value is NIL;
    var string: objclass is "";
*)
  begin
    type_obj := match_obj(aType);
    if not type_obj in declared_types then

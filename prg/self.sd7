$ include "seed7_05.s7i";
const array string: prog is [](
"include \"seed7_05.s7i\";",
"const array string: prog is [](",
"const proc: main is func",
"  local var integer: number is 0;",
"  begin",
"    for number range 1 to 2 do writeln(prog[number]); end for;",
"    for number range 1 to 12 do",
"      write(literal(prog[number])); writeln(\",\");",
"    end for;",
"    write(literal(prog[13])); writeln(\");\");",
"    for number range 3 to 13 do writeln(prog[number]); end for;",
"  end func;",
"$ system \"main\" is main;");
const proc: main is func
  local var integer: number is 0;
  begin
    for number range 1 to 2 do writeln(prog[number]); end for;
    for number range 1 to 12 do
      write(literal(prog[number])); writeln(",");
    end for;
    write(literal(prog[13])); writeln(");");
    for number range 3 to 13 do writeln(prog[number]); end for;
  end func;
$ system "main" is main;

const func boolean: chkPeekBitsMsb (in string: stri, in integer: bytePos,
    in integer: bitPos, in integer: bitsNeeded, in integer: bitsExpected) is func
  result
    var boolean: okay is TRUE;
  local
    var integer: bits is 0;
    var char: ch is ' ';
    var file: testFile is STD_NULL;
    var msbInBitStream: aBitStream is msbInBitStream.value;
  begin
    # Test msbInBitStream with string:
    aBitStream := openMsbBitStream(stri);
    skipBits(aBitStream, 8 * pred(bytePos) + bitPos);
    bits := peekBits(aBitStream, bitsNeeded);
    if bits <> bitsExpected then
      okay := FALSE;
      write("\"");
      for ch range stri do
        write("\\2#" <& ord(ch) radix 2 <& ";");
      end for;
      writeln("\": peekBits(msbInBitStream, " <& bitsNeeded <& ")");
      writeln(" ***** Expected " <& bitsExpected <& " found " <& bits);
    end if;

    # Test msbInBitStream with file:
    testFile := openStriFile(stri);
    aBitStream := openMsbBitStream(testFile);
    skipBits(aBitStream, 8 * pred(bytePos) + bitPos);
    bits := peekBits(aBitStream, bitsNeeded);
    if bits <> bitsExpected then
      okay := FALSE;
      write("\"");
      for ch range stri do
        write("\\2#" <& ord(ch) radix 2 <& ";");
      end for;
      writeln("\": peekBits(msbInBitStream, " <& bitsNeeded <& ")");
      writeln(" ***** Expected " <& bitsExpected <& " found " <& bits);
    end if;
  end func;



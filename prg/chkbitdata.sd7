const func boolean: chkGetsLsb (in string: stri, in integer: bytePos,
    in integer: bitPos, in integer: bytesNeeded, in string: bytesExpected) is func
  result
    var boolean: okay is TRUE;
  local
    var string: bytes is "";
    var char: ch is ' ';
    var file: testFile is STD_NULL;
    var lsbInBitStream: aBitStream is lsbInBitStream.value;
  begin
    # Test lsbInBitStream with string:
    aBitStream := openLsbInBitStream(stri);
    skipBits(aBitStream, 8 * pred(bytePos) + bitPos);
    bytes := gets(aBitStream, bytesNeeded);
    if bytes <> bytesExpected or getBits(aBitStream, 4) <> 2#1101 then
      okay := FALSE;
      write("\"");
      for ch range stri do
        write("\\2#" <& ord(ch) radix 2 <& ";");
      end for;
      writeln("\": gets(lsbInBitStream, " <& bytesNeeded <& ")");
      if bytes <> bytesExpected then
        writeln(" ***** Expected " <& literal(bytesExpected) <& " found " <& literal(bytes));
      else
        writeln(" ***** Position afterwards not okay.");
      end if;
    end if;

    # Test lsbInBitStream with file:
    testFile := openStriFile(stri);
    aBitStream := openLsbInBitStream(testFile);
    skipBits(aBitStream, 8 * pred(bytePos) + bitPos);
    bytes := gets(aBitStream, bytesNeeded);
    if bytes <> bytesExpected or getBits(aBitStream, 4) <> 2#1101 then
      okay := FALSE;
      write("\"");
      for ch range stri do
        write("\\2#" <& ord(ch) radix 2 <& ";");
      end for;
      writeln("\": gets(lsbInBitStream, " <& bytesNeeded <& ")");
      if bytes <> bytesExpected then
        writeln(" ***** Expected " <& literal(bytesExpected) <& " found " <& literal(bytes));
      else
        writeln(" ***** Position afterwards not okay.");
      end if;
    end if;
  end func;



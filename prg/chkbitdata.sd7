const func boolean: chkLengthTruncateLsb (in string: stri, in integer: bitPos,
    in integer: oldLength, in integer: newLength, in string: striExpected,
    in integer: bitPosExpected) is func
  result
    var boolean: okay is TRUE;
  local
    var lsbOutBitStream: aBitStream is lsbOutBitStream.value;
    var string: bytes is "";
  begin
    initBitStream(aBitStream, stri, bitPos);
    if length(aBitStream) <> oldLength then
      writeln(" ***** Expected length " <& oldLength <& " found " <& length(aBitStream));
      okay := FALSE;
    end if;
    truncate(aBitStream, newLength);
    if length(aBitStream) <> newLength then
      writeln(" ***** Expected length " <& newLength <& " found " <& length(aBitStream));
      okay := FALSE;
    end if;
    if aBitStream.bitPos <> bitPosExpected then
      writeln(" ***** Expected bitPos " <& bitPosExpected <& " found " <& aBitStream.bitPos);
      okay := FALSE;
    end if;
    flush(aBitStream);
    bytes := getBytes(aBitStream);
    if bytes <> striExpected then
      writeln(" ***** Expected " <& literal(striExpected) <& " found " <& literal(bytes));
      okay := FALSE;
    end if;
  end func;



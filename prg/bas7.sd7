        log << symbol;
        log << NL;
        flush(log);
        symbol := "";
        result := FALSE;
      elsif symbol = "SWAP" then
        symbol := get_symbol(line);
        if symbol[length(symbol)] = '$' or symbol[1] in defstr_var then
          variable_name := get_name(symbol, line);
          expect(",", symbol, line);
          if symbol[length(symbol)] = '$' or symbol[1] in defstr_var then
            variable2_name := get_name(symbol, line);
            param1 := string_var[variable_name];
            param2 := string_var[variable2_name];
            string_var @:= [variable_name] param2;
            string_var @:= [variable2_name] param1;
          else

      else
        error_marker;
        err << "STRING VARIABLE EXPECTED IN LINE INPUT FOUND ";
        err << literal(symbol);
        err << ".";
        err << NL;
      end if;
    else
      error_marker;
      err << "FILE #";
      err << index1;
      err << " NOT OPEN IN LINE INPUT.";

            num1 := numeric_var[variable_name];
            num2 := numeric_var[variable2_name];
            numeric_var @:= [variable_name] num2;
            numeric_var @:= [variable2_name] num1;
          else
            error_marker;
            err << "NUMERIC VARIABLE EXPECTED - FOUND ";
            err << literal(symbol);
            err << ".";
            err << NL;
          end if;
        end if;
      elsif symbol = "CALL" then
        symbol := get_symbol(line);
        if symbol in subprogram then
          line_marker;
          log << "CALL ";
          log << symbol;
          log << NL;
          flush(log);
          advance_after_statement(line);
          set_return_position(line);
          file_line_number := subprogram[symbol];
          statement_label := prg[file_line_number].label;
          line := prg[file_line_number].line;
          symbol := get_symbol(line);
          symbol := get_symbol(line);
          set_sub_entry_position;
          line_marker;
          log << "EXECUTE SUB ";
          log << symbol;
          log << NL;
        else
          error_marker;
          err << "CALL UNDEFINED SUB ";
          err << literal(symbol);
          err << ".";
          err << NL;
        end if;
        while not endOfStatement(symbol) do
          symbol := get_symbol(line);
        end while;
      elsif symbol in subprogram then
        line_marker;
        log << "CALL ";
        log << symbol;
        log << NL;
        flush(log);
        advance_after_statement(line);
        set_return_position(line);
        file_line_number := subprogram[symbol];
        statement_label := prg[file_line_number].label;
        line := prg[file_line_number].line;
        symbol := get_symbol(line);
        symbol := get_symbol(line);
        set_sub_entry_position;
        line_marker;
        log << "EXECUTE SUB ";
        log << symbol;
        log << NL;
        while not endOfStatement(symbol) do
          symbol := get_symbol(line);
        end while;
      elsif symbol = "CONST" then
        symbol := get_symbol(line);
        if symbol[length(symbol)] = '$' or symbol[1] in defstr_var then

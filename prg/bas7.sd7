        index1 := round(exec_expr(symbol, line));
        expect(",", symbol, line);
        index2 := round(exec_expr(symbol, line));
        line_marker;
        log << "**OUT ";
        log << index1;
        log << ", ";
        log << index2;
        log << NL;
        flush(log);
      elsif symbol = "SUB" then
        symbol := get_symbol(line);
        line_marker;
        log << "SKIP SUB ";
        log << symbol;
        log << NL;
        flush(log);
        symbol := find_end_sub(line);
        symbol := get_symbol(line);
        line_marker;
        log << "END SKIP SUB ";
        log << symbol;
        log << " ";
        log << line;
        log << NL;
        flush(log);
      elsif symbol = "ERASE" then
        line_marker;
        log << "**ERASE ";
        log << line;
        log << NL;
        flush(log);
        repeat
          symbol := get_symbol(line);
        until endOfStatement(symbol);
      elsif symbol = "COMMON" then
        line_marker;
        log << "**COMMON ";
        log << line;
        log << NL;
        flush(log);
        repeat
          symbol := get_symbol(line);
        until endOfStatement(symbol);
      elsif symbol = "SLEEP" then
        symbol := get_symbol(line);
        if endOfStatement(symbol) then
          index1 := 1;
        else
          index1 := round(exec_expr(symbol, line));
        end if;
        flush(win);
        sleep(index1);
        line_marker;
        log << "SLEEP ";
        log << index1;
        log << NL;
        flush(log);
      elsif symbol = "STOP" or symbol = "SYSTEM" then

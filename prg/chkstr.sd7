const proc: check_string_unicode_conversons is func
  local
    const string: utf16BeTest is "\0;a\0;b\0;c\0;\0;\0;d\0;e\0;f\0;\0; ¬\0;\0;Ø4Ý\30;\0;\0;zyxwvuts";
    const string: utf16LeTest is "a\0;b\0;c\0;\0;\0;d\0;e\0;f\0;\0;\0;¬ \0;\0;4Ø\30;Ý\0;\0;zyxwvuts";
    var file: testFile is STD_NULL;
    var boolean: okay is TRUE;
    var integer: pos is 1;
  begin
    if not check_utf8_conversions then
      okay := FALSE;
    end if;

    if  toUtf16Be("abc")                                       <> "\0;a\0;b\0;c" or
        toUtf16Be("€")                                         <> " ¬" or
        toUtf16Be("𝄞")                                         <> "Ø4Ý\30;" or
        toUtf16Be("\119070;")                                  <> "Ø4Ý\30;" or
        toUtf16Be("\16#10ffff;")                               <> "Ûÿßÿ" or
        toUtf16Le("abc")                                       <> "a\0;b\0;c\0;" or
        toUtf16Le("€")                                         <> "¬ " or
        toUtf16Le("𝄞")                                         <> "4Ø\30;Ý" or
        toUtf16Le("\119070;")                                  <> "4Ø\30;Ý" or
        toUtf16Le("\16#10ffff;")                               <> "ÿÛÿß" or
        fromUtf16Be("\0;a\0;b\0;c")                            <> "abc" or
        fromUtf16Be(" ¬")                                      <> "€" or
        fromUtf16Be("Ø4Ý\30;")                                 <> "𝄞" or
        fromUtf16Be("\16#d8;\16#34;\16#dd;\16#1e;")            <> "𝄞" or
        fromUtf16Le("a\0;b\0;c\0;")                            <> "abc" or
        fromUtf16Le("¬ ")                                      <> "€" or
        fromUtf16Le("4Ø\30;Ý")                                 <> "𝄞" or
        fromUtf16Le("\16#34;\16#d8;\16#1e;\16#dd;")            <> "𝄞" or
        replaceUtf16SurrogatePairs("\16#d834;\16#dd1e;")       <> "𝄞" or
        replaceUtf16SurrogatePairs("abc\16#d834;\16#dd1e;def") <> "abc𝄞def" then
      writeln(" ***** UTF-16 Unicode conversions do not work correctly.");
      okay := FALSE;
    end if;

    if  fromNullTerminatedUtf16Be(utf16BeTest,  1) <> "abc" or
        fromNullTerminatedUtf16Be(utf16BeTest,  9) <> "def" or
        fromNullTerminatedUtf16Be(utf16BeTest, 17) <> "€" or
        fromNullTerminatedUtf16Be(utf16BeTest, 21) <> "𝄞" then
      writeln(" ***** fromNullTerminatedUtf16Be(string, pos) does not work correctly.");
      okay := FALSE;
    end if;

    if  fromNullTerminatedUtf16Le(utf16LeTest,  1) <> "abc" or
        fromNullTerminatedUtf16Le(utf16LeTest,  9) <> "def" or
        fromNullTerminatedUtf16Le(utf16LeTest, 17) <> "€" or
        fromNullTerminatedUtf16Le(utf16LeTest, 21) <> "𝄞" then
      writeln(" ***** fromNullTerminatedUtf16Le(string, pos) does not work correctly.");
      okay := FALSE;
    end if;

    if  getNullTerminatedUtf16Be(utf16BeTest, pos) <> "abc" or
        getNullTerminatedUtf16Be(utf16BeTest, pos) <> "def" or
        getNullTerminatedUtf16Be(utf16BeTest, pos) <> "€" or
        getNullTerminatedUtf16Be(utf16BeTest, pos) <> "𝄞" then
      writeln(" ***** getNullTerminatedUtf16Be(string, pos) does not work correctly.");
      okay := FALSE;
    end if;

    pos := 1;
    if  getNullTerminatedUtf16Le(utf16LeTest, pos) <> "abc" or
        getNullTerminatedUtf16Le(utf16LeTest, pos) <> "def" or
        getNullTerminatedUtf16Le(utf16LeTest, pos) <> "€" or
        getNullTerminatedUtf16Le(utf16LeTest, pos) <> "𝄞" then
      writeln(" ***** getNullTerminatedUtf16Le(string, pos) does not work correctly.");
      okay := FALSE;
    end if;

    testFile := openStriFile(utf16BeTest);
    if  getNullTerminatedUtf16Be(testFile) <> "abc" or
        getNullTerminatedUtf16Be(testFile) <> "def" or
        getNullTerminatedUtf16Be(testFile) <> "€" or
        getNullTerminatedUtf16Be(testFile) <> "𝄞" then
      writeln(" ***** getNullTerminatedUtf16Be(file) does not work correctly.");
      okay := FALSE;
    end if;

    testFile := openStriFile(utf16LeTest);
    if  getNullTerminatedUtf16Le(testFile) <> "abc" or
        getNullTerminatedUtf16Le(testFile) <> "def" or
        getNullTerminatedUtf16Le(testFile) <> "€" or
        getNullTerminatedUtf16Le(testFile) <> "𝄞" then
      writeln(" ***** getNullTerminatedUtf16Le(file) does not work correctly.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(toUtf16Be("\16#d800;")) or
        not raisesRangeError(toUtf16Be("\16#dfff;")) or
        not raisesRangeError(toUtf16Be("\16#110000;")) or
        not raisesRangeError(toUtf16Le("\16#d800;")) or
        not raisesRangeError(toUtf16Le("\16#dfff;")) or
        not raisesRangeError(toUtf16Le("\16#110000;")) or
        not raisesRangeError(fromUtf16Be("abc")) or
        not raisesRangeError(fromUtf16Be("ab\256;c")) or
        not raisesRangeError(fromUtf16Be("abc\256;")) or
        not raisesRangeError(fromUtf16Be("\16#dd;\16#1e;")) or
        not raisesRangeError(fromUtf16Be("\16#d8;\16#34;")) or
        not raisesRangeError(fromUtf16Be("\16#d8;\16#34;\256;a")) or
        not raisesRangeError(fromUtf16Be("\16#d8;\16#34;a\256;")) or
        not raisesRangeError(fromUtf16Be("\16#d8;\16#34;\16#db;\16#ff;")) or
        not raisesRangeError(fromUtf16Be("\16#d8;\16#34;\16#e0;\16#00;")) or
        not raisesRangeError(fromUtf16Le("abc")) or
        not raisesRangeError(fromUtf16Le("ab\256;c")) or
        not raisesRangeError(fromUtf16Le("abc\256;")) or
        not raisesRangeError(fromUtf16Le("\16#1e;\16#dd;")) or
        not raisesRangeError(fromUtf16Le("\16#34;\16#d8;")) or
        not raisesRangeError(fromUtf16Le("\16#34;\16#d8;\256;a")) or
        not raisesRangeError(fromUtf16Le("\16#34;\16#d8;a\256;")) or
        not raisesRangeError(fromUtf16Le("\16#34;\16#d8;\16#ff;\16#db;")) or
        not raisesRangeError(fromUtf16Le("\16#34;\16#d8;\16#00;\16#e0;")) or
        not raisesRangeError(replaceUtf16SurrogatePairs("\16#dd1e;")) or
        not raisesRangeError(replaceUtf16SurrogatePairs("\16#d834;")) or
        not raisesRangeError(replaceUtf16SurrogatePairs("\16#d834;a")) then
      writeln(" ***** Wrong UTF-16 Unicode conversions does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if okay then
      writeln("Unicode conversons work correctly.");
    end if;
  end func;



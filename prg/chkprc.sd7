  end enum;

var boolean: global_loop_control is TRUE;

var boolean: true_var is TRUE;
var boolean: false_var is FALSE;


const func boolean: boolExpr (in boolean: aBool) is func
  result
    var boolean: boolResult is FALSE;
  begin
    boolResult := aBool;
  end func;


const func enumerationType: enumExpr (in enumerationType: aValue) is
  return enumerationType conv (ord(aValue) + length(str(rand(1, 9))[2 ..]));


const func string: striExpr (in string: stri) is
  return stri & str(rand(1, 9))[2 ..];


const func bstring: bstriExpr (in bstring: bstri) is
  return bstring(str(bstri) & str(rand(1, 9))[2 ..]);


const func integer: intExpr (in integer: number) is
  return number + length(str(rand(1, 9))[2 ..]);


const func char: charExpr (in char: ch) is
  return char(ord(ch) + length(str(rand(1, 9))[2 ..]));


const func bigInteger: bigintExpr (in bigInteger: number) is func
  result
    var bigInteger: exprResult is 0_;
  begin
    exprResult := number;
  end func;


const func float: floatExpr (in float: number) is
  return number;


const func rational: rationalExpr (in rational: number) is
  return intExpr(number.numerator) / intExpr(number.denominator);


const func bitset: setExpr (in bitset: aSet) is
  return aSet;


const func type: typeExpr (in type: aValue) is
  return aValue;



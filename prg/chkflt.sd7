    number := rand(     -2.0,      -1.0); ok := ok and number >= -2.0 and number < -1.0;
    number := rand(     -2.0,       0.0); ok := ok and number >= -2.0 and number <  0.0;
    number := rand(     -2.0,       1.0); ok := ok and number >= -2.0 and number <  1.0;
    number := rand(     -2.0,       2.0); ok := ok and number >= -2.0 and number <  2.0;
    number := rand(     -2.0,  Infinity); ok := ok and number >= -2.0 and number <= Infinity;
    number := rand(     -1.0,       0.0); ok := ok and number >= -1.0 and number < 0.0;
    number := rand(     -1.0,       1.0); ok := ok and number >= -1.0 and number < 1.0;
    number := rand(     -1.0,       2.0); ok := ok and number >= -1.0 and number < 2.0;
    number := rand(     -1.0,  Infinity); ok := ok and number >= -1.0 and number <= Infinity;
    number := rand(      0.0,       1.0); ok := ok and number >= 0.0 and number < 1.0;
    number := rand(      0.0,       2.0); ok := ok and number >= 0.0 and number < 2.0;
    number := rand(      0.0,  Infinity); ok := ok and number >= 0.0 and number <= Infinity;
    number := rand(      1.0,       2.0); ok := ok and number >= 1.0 and number < 2.0;
    number := rand(      1.0,  Infinity); ok := ok and number >= 1.0 and number <= Infinity;
    number := rand(      2.0,  Infinity); ok := ok and number >= 2.0 and number <= Infinity;
    if not ok then
      writeln(" ***** rand(a, b) does not work correct.");
      okay := FALSE;
    end if;

    ok := TRUE;
    number := rand(0.0, 2.0 ** 1000); ok := ok and number >= 0.0 and number < 2.0 ** 1000;
    number := rand(0.0, 2.0 **  500); ok := ok and number >= 0.0 and number < 2.0 **  500;
    number := rand(0.0, 2.0 **  200); ok := ok and number >= 0.0 and number < 2.0 **  200;
    number := rand(0.0, 1267650600228229401496703205376.0); ok := ok and number >= 0.0 and number < 1267650600228229401496703205376.0;
    number := rand(0.0,             1152921504606846976.0); ok := ok and number >= 0.0 and number < 1152921504606846976.0;
    number := rand(0.0,                   1099511627776.0); ok := ok and number >= 0.0 and number < 1099511627776.0;
    number := rand(0.0,                      1073741824.0); ok := ok and number >= 0.0 and number < 1073741824.0;
    number := rand(0.0,                         1048576.0); ok := ok and number >= 0.0 and number < 1048576.0;

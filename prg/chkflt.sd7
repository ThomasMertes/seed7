    number := rand(0.0,                            1024.0); ok := ok and number >= 0.0 and number < 1024.0;
    number := rand(0.0,                               4.0); ok := ok and number >= 0.0 and number < 4.0;
    number := rand(0.0,                               2.0); ok := ok and number >= 0.0 and number < 2.0;
    number := rand(0.0,                               1.0); ok := ok and number >= 0.0 and number < 1.0;
    number := rand(0.0, 0.5);                           ok := ok and number >= 0.0 and number < 0.5;
    number := rand(0.0, 0.25);                          ok := ok and number >= 0.0 and number < 0.25;
    number := rand(0.0, 0.125);                         ok := ok and number >= 0.0 and number < 0.125;
    number := rand(0.0, 0.0625);                        ok := ok and number >= 0.0 and number < 0.0625;
    number := rand(0.0, 0.03125);                       ok := ok and number >= 0.0 and number < 0.03125;
    number := rand(0.0, 0.015625);                      ok := ok and number >= 0.0 and number < 0.015625;
    number := rand(0.0, 0.0078125);                     ok := ok and number >= 0.0 and number < 0.0078125;
    number := rand(0.0, 0.0009765625);                  ok := ok and number >= 0.0 and number < 0.0009765625;
    number := rand(0.0, 0.000030517578125);             ok := ok and number >= 0.0 and number < 0.000030517578125;
    number := rand(0.0, 0.00000095367431640625);        ok := ok and number >= 0.0 and number < 0.00000095367431640625;
    number := rand(0.0, 0.000000000931322574615479);    ok := ok and number >= 0.0 and number < 0.000000000931322574615479;
    number := rand(0.0, 0.000000000000909494701772928); ok := ok and number >= 0.0 and number < 0.000000000000909494701772928;
    number := rand(0.0, 2.0 ** ( -100)); ok := ok and number >= 0.0 and number < 2.0 ** ( -100);
    number := rand(0.0, 2.0 ** ( -200)); ok := ok and number >= 0.0 and number < 2.0 ** ( -200);
    number := rand(0.0, 2.0 ** ( -500)); ok := ok and number >= 0.0 and number < 2.0 ** ( -500);
    number := rand(0.0, 2.0 ** (-1000)); ok := ok and number >= 0.0 and number < 2.0 ** (-1000);
    if not ok then
      writeln(" ***** rand(0.0, upper) does not work correct if upper is a power of two.");
      okay := FALSE;
    end if;

    for 10000 do
      number := rand(-Infinity, Infinity);
      if isNaN(number) then
        nanPresent := TRUE;
      elsif number < 0.0 then
        negativePresent := TRUE;
      elsif number > 0.0 then
        positivePresent := TRUE;
      end if;
    end for;

    if nanPresent then
      writeln(" ***** rand(-Infinity, Infinity) returns NaN.");
      okay := FALSE;
    end if;
    if not negativePresent then
      writeln(" ***** rand(-Infinity, Infinity) returns only positive values.");
      okay := FALSE;
    end if;
    if not positivePresent then
      writeln(" ***** rand(-Infinity, Infinity) returns only negative values.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(rand(          -Infinity,            -Infinity )) or
        not raisesRangeError(rand(          -Infinity,  floatExpr(-Infinity))) or
        not raisesRangeError(rand(floatExpr(-Infinity),           -Infinity )) or
        not raisesRangeError(rand(floatExpr(-Infinity), floatExpr(-Infinity))) or
        not raisesRangeError(rand(               -2.0,                 -2.0 )) or
        not raisesRangeError(rand(               -2.0,  floatExpr(     -2.0))) or
        not raisesRangeError(rand(floatExpr(     -2.0),                -2.0 )) or
        not raisesRangeError(rand(floatExpr(     -2.0), floatExpr(     -2.0))) or
        not raisesRangeError(rand(               -1.0,                 -1.0 )) or
        not raisesRangeError(rand(               -1.0,  floatExpr(     -1.0))) or
        not raisesRangeError(rand(floatExpr(     -1.0),                -1.0 )) or
        not raisesRangeError(rand(floatExpr(     -1.0), floatExpr(     -1.0))) or
        not raisesRangeError(rand(                0.0,                  0.0 )) or
        not raisesRangeError(rand(                0.0,  floatExpr(      0.0))) or
        not raisesRangeError(rand(floatExpr(      0.0),                 0.0 )) or
        not raisesRangeError(rand(floatExpr(      0.0), floatExpr(      0.0))) or
        not raisesRangeError(rand(                1.0,                  1.0 )) or
        not raisesRangeError(rand(                1.0,  floatExpr(      1.0))) or
        not raisesRangeError(rand(floatExpr(      1.0),                 1.0 )) or
        not raisesRangeError(rand(floatExpr(      1.0), floatExpr(      1.0))) or
        not raisesRangeError(rand(                2.0,                  2.0 )) or
        not raisesRangeError(rand(                2.0,  floatExpr(      2.0))) or
        not raisesRangeError(rand(floatExpr(      2.0),                 2.0 )) or
        not raisesRangeError(rand(floatExpr(      2.0), floatExpr(      2.0))) or
        not raisesRangeError(rand(           Infinity,             Infinity )) or
        not raisesRangeError(rand(           Infinity,  floatExpr( Infinity))) or
        not raisesRangeError(rand(floatExpr( Infinity),            Infinity )) or
        not raisesRangeError(rand(floatExpr( Infinity), floatExpr( Infinity))) then
      writeln(" ***** rand(0.0, 0.0) does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(rand(          1.0,            0.0 )) or
        not raisesRangeError(rand(          1.0,  floatExpr(0.0))) or
        not raisesRangeError(rand(floatExpr(1.0),           0.0 )) or
        not raisesRangeError(rand(floatExpr(1.0), floatExpr(0.0))) then
      writeln(" ***** rand(1.0, 0.0) does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if  not raisesRangeError(rand(          0.0,            NaN )) or
        not raisesRangeError(rand(          NaN,            1.0 )) or
        not raisesRangeError(rand(          NaN,            NaN )) or
        not raisesRangeError(rand(          0.0,  floatExpr(NaN))) or
        not raisesRangeError(rand(          NaN,  floatExpr(1.0))) or
        not raisesRangeError(rand(          NaN,  floatExpr(NaN))) or
        not raisesRangeError(rand(floatExpr(0.0),           NaN )) or
        not raisesRangeError(rand(floatExpr(NaN),           1.0 )) or
        not raisesRangeError(rand(floatExpr(NaN),           NaN )) or
        not raisesRangeError(rand(floatExpr(0.0), floatExpr(NaN))) or
        not raisesRangeError(rand(floatExpr(NaN), floatExpr(1.0))) or
        not raisesRangeError(rand(floatExpr(NaN), floatExpr(NaN))) then
      writeln(" ***** rand with NaN as upper or lower bound does not raise RANGE_ERROR.");
      okay := FALSE;
    end if;

    if okay then
      writeln("Rand works correct for selected values.");
    else
      writeln(" ***** Rand does not work correct.");
    end if;

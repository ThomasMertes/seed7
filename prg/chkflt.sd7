const proc: check_rand is func

  local
    var float: number is 0.0;
    var boolean: nanPresent is FALSE;
    var boolean: negativePresent is FALSE;
    var boolean: positivePresent is FALSE;
    var boolean: okay is TRUE;
    var boolean: ok is TRUE;
  begin
    number := rand(-Infinity,      -2.0); ok := ok and number >= -Infinity and number < -2.0;
    number := rand(-Infinity,      -1.0); ok := ok and number >= -Infinity and number < -1.0;
    number := rand(-Infinity,       0.0); ok := ok and number >= -Infinity and number <  0.0;
